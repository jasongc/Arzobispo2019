
<div class="container-fluid" style="padding:0px;">
    <div class="box box-info mt-2 mb-0" id="acordion">
        <div class="box-header with-border" id="heading1">
            <h3 class="box-title" style="cursor:pointer;"><i class="icon-list-numbered"></i>Lista de Exámenes</h3>
        </div>
    </div>
</div>
<input id="heightBoardProd" type="hidden" value="">
<div id="contBoardCallingPacient" class="overflow-hidden p-1" style="width:100% !important; box-shadow: -5px 5px 8px #bebcbc; border-radius:0.3rem">
    <div class="box box-info mt-2 mb-0">
        <div class="scroll-css" style="overflow:scroll; height: 300px;">
            <table class="table table-sm table-hover" id="table-examenes">
                <thead>
                    <tr class="text-center">
                        <th nowrap scope="col" class="d-none">CategoryId</th>
                        <th nowrap scope="col" class="d-none">ServiceId</th>
                        <th nowrap scope="col">Examen</th>
                        <th nowrap scope="col">Estado Cola</th>
                        <th nowrap scope="col">Estado Componente</th>
                        <th nowrap scope="col">Fecha de Inicio</th>
                        <th nowrap scope="col">Fecha de Fin</th>
                        <th nowrap scope="col" class="d-none">ServiceComponentId</th>
                        <th nowrap scope="col" class="d-none">PersonId</th>
                    </tr>
                </thead>
                <tbody>
                    @{

                        if (ViewBag.ExamsPacient == null)
                        {
                            <tr><td colspan="16" class="text-center">Sin resultados</td></tr>
                        }
                        else if (ViewBag.ExamsPacient.List.Count == 0)
                        {
                            <tr><td colspan="16" class="text-center">No hay resultados</td></tr>
                        }
                        else
                        {
                            foreach (var item in ViewBag.ExamsPacient.List)
                            {
                            <tr class="text-center cmExamenes">
                                <td nowrap class="d-none">@item.i_CategoryId</td>
                                <td nowrap class="d-none">@item.v_ServiceId</td>
                                <td nowrap>@item.v_CategoryName</td>
                                <td nowrap>@item.v_QueueStatusName</td>
                                <td nowrap>@item.v_ServiceComponentStatusName</td>
                                <td nowrap>@item.d_StartDate</td>
                                <td nowrap>@item.d_EndDate</td>
                                <td nowrap class="d-none">@item.v_PersonId</td>
                            </tr>
                            }
                        }
                    }

                </tbody>
            </table>
        </div>
        
    </div>
</div>
<script>

</script>




